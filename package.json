{
  "name": "react-openapi-designer",
  "version": "0.13.0",
  "main": "lib/bundle",
  "homepage": "https://codeasashu.github.io/react-openapi-designer/",
  "repository": {
    "type": "git",
    "url": "https://github.com/codeasashu/react-openapi-designer"
  },
  "scripts": {
    "typecheck": "flow check",
    "lint": "eslint --ignore-path .gitignore  --ignore-path .prettierignore .",
    "prettier": "prettier --ignore-path .gitignore --ignore-path .prettierignore \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:type": "npm run typecheck && npm run jest",
    "build": "NODE_ENV=production webpack --config webpack/webpack.prod.js --progress",
    "bundle:lib": "NODE_ENV=production webpack --config webpack/webpack.lib.js --progress",
    "bundle:browser": "NODE_ENV=production webpack --config webpack/webpack.prod.js --progress",
    "watch": "webpack --config webpack/webpack.common.js --watch  --progress",
    "dev": "webpack serve -c webpack/webpack.dev.js --hot",
    "validate": "npm-run-all --parallel format:check lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "@blueprintjs/core": "^4.9.3",
    "@blueprintjs/popover2": "^1.6.3",
    "@blueprintjs/select": "^4.6.3",
    "@cloudflare/json-schema-walker": "^0.1.1",
    "@stoplight/elements-core": "^7.5.6",
    "@stoplight/http-spec": "^4.3.0",
    "@stoplight/path": "^1.3.2",
    "@stoplight/spectral-cli": "^6.1.0",
    "class-autobind-decorator": "^3.0.1",
    "classnames": "^2.3.1",
    "highlight.js": "^11.3.1",
    "immer": "^9.0.6",
    "js-yaml": "^4.1.0",
    "json-schema-faker": "^0.5.0-rcv.33",
    "lodash": "^4.17.20",
    "lodash.isequal": "^4.5.0",
    "monaco-editor": "^0.27.0",
    "openapi3-ts": "^2.0.1",
    "postman-to-openapi": "github:codeasashu/postman-to-openapi#browser",
    "prop-types": "^15.7.2",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-input-autosize": "^3.0.0",
    "react-lifecycles-compat": "^3.0.4",
    "react-simple-code-editor": "^0.13.0",
    "react-tabs": "^3.2.2",
    "react-window": "^1.8.6",
    "redoc": "^2.0.0-rc.58",
    "styled-components": "^5.3.1",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "type-of-is": "^3.5.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./__jest__/setup-after-env.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": [
        "babel-jest",
        {
          "configFile": "./babel-jest.config.js"
        }
      ],
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(monaco-editor|react-monaco-editor)/)"
    ],
    "verbose": false,
    "resetMocks": true,
    "resetModules": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "clover",
      "html"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/dom": "^8.11.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^14",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "buffer": "^6.0.3",
    "css-loader": "^5.1.1",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-testing-library": "^5.7.0",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.152.0",
    "flow-typed": "^3.3.1",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.2.0",
    "https-browserify": "^1.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-extended": "^1.1.0",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "monaco-editor-webpack-plugin": "^4.1.2",
    "npm-run-all": "^4.1.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.2.7",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^5.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^3.1.8",
    "terser-webpack-plugin": "^5.1.1",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "react": "^16.8.4 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.4 || ^17.0.0 || ^18.0.0",
    "styled-components": "^4.1.1 || ^5.1.1"
  }
}
