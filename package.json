{
  "name": "react-openapi-designer",
  "version": "0.1.1",
  "main": "dist/main.js",
  "scripts": {
    "typecheck": "flow check",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:type": "npm run typecheck && npm run jest",
    "build": "NODE_ENV=production webpack --config webpack/webpack.prod.js --progress",
    "watch": "webpack --config webpack/webpack.common.js --watch  --progress",
    "dev": "webpack serve -c webpack/webpack.dev.js --hot --open",
    "validate": "npm-run-all --parallel format:check lint && npm run build"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.40.0",
    "@blueprintjs/popover2": "^0.3.1",
    "@blueprintjs/select": "^3.15.8",
    "@reduxjs/toolkit": "^1.5.0",
    "class-autobind-decorator": "^3.0.1",
    "classnames": "^2.3.1",
    "json-schema-faker": "^0.5.0-rcv.33",
    "lodash": "^4.17.20",
    "openapi3-ts": "^2.0.1",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.2.0",
    "react-simple-code-editor": "^0.11.0",
    "react-tabs": "^3.2.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0",
    "tailwindcss": "^2.0.3",
    "type-of-is": "^3.5.1"
  },
  "peerDependencies": {
    "react": ">=16.13"
  },
  "eslintIgnore": [
    "*.test.js"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./__jest__/setup-after-env.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "verbose": false,
    "resetMocks": true,
    "resetModules": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "clover",
      "html"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-prismjs": "^2.0.1",
    "buffer": "^6.0.3",
    "css-loader": "^5.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-react": "^7.23.1",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.152.0",
    "flow-typed": "^3.3.1",
    "html-webpack-plugin": "^5.2.0",
    "https-browserify": "^1.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-popper": "^1.3.11",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "stream-http": "^3.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
